{
  "name": "@mastra/voice",
  "version": "0.1.0",
  "description": "TypeScript framework for building voice and multimodal conversational agents",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:update": "vitest run -u",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@types/node": "^20.0.0",
    "@types/ws": "^8.5.10",
    "mediasoup-client": "^3.7.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^1.3.1",
    "@vitest/ui": "^1.3.1",
    "eslint": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.3.1"
  }
}
